{
  "name": "solias",
  "version": "0.0.1-beta.3",
  "description": "Web Component Library made using StencilJS",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/solias/solias.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Buddhilive/solias"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -o docs",
    "watch": "npm run build && npm run storybook"
  },
  "dependencies": {
    "@stencil/core": "^4.5.0",
    "vite": "^4.5.0"
  },
  "devDependencies": {
    "@stencil/sass": "^3.0.5",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/html": "^7.5.1",
    "@storybook/html-vite": "^7.5.1",
    "@storybook/testing-library": "^0.2.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.1",
    "storybook-dark-mode": "^3.0.1"
  },
  "license": "MIT",
  "keywords": [
    "Stenciljs"
  ],
  "bugs": {
    "url": "https://github.com/Buddhilive/solias/issues"
  },
  "homepage": "https://solias.buddhilive.com"
}
